    <AliasPackage>
        <Alias isActive="yes" isFolder="no">
            <name>ConsiderRanksActivator</name>
            <script>enableTrigger(&quot;ConsiderRanks&quot;)
send(matches[1]) -- sends the command exactly as typed
tempTimer( 2, [[disableTrigger(&quot;ConsiderRanks&quot;)]])

--This alias is mainly so that the rank phrases don't trigger
--at the wrong times, such as during conversation.
--If you prefer alternate aliases for PROBE and CONSIDER, just
--add it in the parentheses of the pattern.
--example: ^(p|probe|con|consider|WhatIsThat) .*</script>
            <command></command>
            <packageName></packageName>
            <regex>^(p|probe|con|consider) .*</regex>
        </Alias>
